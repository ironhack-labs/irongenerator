{
  "name": "ironmaker",
  "description": "ironmaker helps you quickly generate a simple Node.js application boilerplate",
  "version": "0.3.1",
  "author": "Jos√© Carneiro <contact@josecarnei.ro>",
  "keywords": [
    "express",
    "framework",
    "web",
    "rest",
    "restful",
    "router",
    "app",
    "api"
  ],
  "repository": "josecarneiro/ironmaker",
  "license": "MIT",
  "main": "bin/cli.js",
  "preferGlobal": true,
  "bin": {
    "ironmaker": "./bin/cli.js"
  },
  "scripts": {
    "dev": "DEVELOPMENT=true ./bin/cli.js && cd app && npm i",
    "dev:clean": "DEVELOPMENT=true rm -rf ./app && ./bin/cli.js && cd app && npm i",
    "lint": "eslint .",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "mocha --reporter spec --check-leaks test/"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "minimatch": "^3.0.4",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.0",
    "prettier": "^2.2.1",
    "sorted-object": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.2.1",
    "supertest": "^4.0.2"
  },
  "files": [
    "LICENSE",
    "bin/",
    "lib/",
    "templates/"
  ]
}
